            <rhs
                xsi:type="ASM:Constant"
                value="1.0"/>
          </actualArgs>
        </instructions>
      </thenPart>
    </instructions>
    <instructions
        xsi:type="ASM:Forward">
      <steps
          xsi:type="ASM:Div">
        <lhs
            xsi:type="ASM:ParameterCall"
            parameter="//@instructions.1/@args.0"/>
        <rhs
            xsi:type="ASM:Constant"
            value="1.5"/>
      </steps>
    </instructions>
    <instructions
        xsi:type="ASM:Left">
      <angle
          xsi:type="ASM:Constant"
          value="45.0"/>
    </instructions>
    <instructions
        xsi:type="ASM:Back">
      <steps
          xsi:type="ASM:ParameterCall"
          parameter="//@instructions.1/@args.0"/>
    </instructions>
    <instructions
        xsi:type="ASM:Left">
      <angle
          xsi:type="ASM:Constant"
          value="90.0"/>
    </instructions>
    <instructions
        xsi:type="ASM:Back">
      <steps
          xsi:type="ASM:ParameterCall"
          parameter="//@instructions.1/@args.0"/>
    </instructions>
    <instructions
        xsi:type="ASM:Right">
      <angle
          xsi:type="ASM:Constant"
          value="90.0"/>
    </instructions>
  </instructions>
  <instructions
      xsi:type="ASM:ProcCall"
      declaration="//@instructions.0">
    <actualArgs
        xsi:type="ASM:Constant"
        value="150.0"/>
  </instructions>
  <instructions
      xsi:type="ASM:ProcCall"
      declaration="//@instructions.1">
    <actualArgs
        xsi:type="ASM:Constant"
        value="150.0"/>
    <actualArgs
        xsi:type="ASM:Constant"/>
  </instructions>
  <instructions
