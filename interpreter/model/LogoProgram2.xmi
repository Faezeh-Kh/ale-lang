        <lhs
            xsi:type="ASM:ParameterCall"
            parameter="//@instructions.1/@args.0"/>
        <rhs
            xsi:type="ASM:Constant"
            value="1.5"/>
      </actualArgs>
    </instructions>
    <instructions
        xsi:type="ASM:If">
      <condition
          xsi:type="ASM:Greater">
        <lhs
            xsi:type="ASM:ParameterCall"
            parameter="//@instructions.1/@args.1"/>
        <rhs
            xsi:type="ASM:Constant"/>
      </condition>
      <thenPart>
        <instructions
            xsi:type="ASM:ProcCall"
            declaration="//@instructions.1">
          <actualArgs
