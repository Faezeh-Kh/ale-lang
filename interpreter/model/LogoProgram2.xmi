            <lhs
                xsi:type="ASM:ParameterCall"
                parameter="//@instructions.1/@args.0"/>
            <rhs
                xsi:type="ASM:Constant"
                value="1.5"/>
          </actualArgs>
          <actualArgs
              xsi:type="ASM:Minus">
            <lhs
                xsi:type="ASM:ParameterCall"
                parameter="//@instructions.1/@args.1"/>
            <rhs
                xsi:type="ASM:Constant"
                value="1.0"/>
          </actualArgs>
        </instructions>
      </thenPart>
    </instructions>
    <instructions
        xsi:type="ASM:Right">
      <angle
          xsi:type="ASM:Constant"
          value="45.0"/>
    </instructions>
    <instructions
        xsi:type="ASM:Forward">
      <steps
          xsi:type="ASM:ParameterCall"
          parameter="//@instructions.1/@args.0"/>
    </instructions>
    <instructions
        xsi:type="ASM:Right">
      <angle
          xsi:type="ASM:Constant"
          value="45.0"/>
    </instructions>
    <instructions
